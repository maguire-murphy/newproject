{
  "name": "@behavioropt/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:sync": "ts-node src/scripts/sync-db.ts",
    "db:sync:force": "ts-node src/scripts/sync-db.ts --force",
    "db:seed": "ts-node src/scripts/seed-database.ts",
    "test:auth": "ts-node src/scripts/test-auth.ts",
    "test:full-auth": "ts-node src/scripts/test-full-auth.ts",
    "generate:secrets": "node src/scripts/generate-secrets.js",
    "setup": "npm run db:sync && npm run db:seed",
    "reset": "npm run db:sync:force && npm run db:seed",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@behavioral-economics-saas/shared": "*",
    "@supabase/supabase-js": "^2.55.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "stripe": "^14.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
